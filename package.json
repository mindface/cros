{
  "name": "cros",
  "version": "1.0.0",
  "main": "public/index.js",
  "license": "MIT",
  "scripts": {
    "build": "node --require esbuild-register build.ts",
    "b": "NODE_ENV=\"devevlopment\" ts-node build.ts",
    "compile": "tsc",
    "dev": "concurrently -c \"bgCyan.bold,bgGreen.bold,bgGrey.bold\" -n \"esbuild,BrowserSync,Express\" \"npm run build:watch\" \"npm run browser:reload\" \"npm run serve:local\"",
    "build:watch": "WATCH=true npm run build",
    "browser:reload": "wait-on public/index.js && browser-sync start --notify=false --proxy 'http://localhost:3030' --files 'public/*' --port 3000",
    "serve:local": "node --require esbuild-register local-hosting.ts",
    "metafile": "NODE_ENV=production META_FILE=true npm run b",
    "css": "npx tailwindcss -i ./src/app.css -o ./public/app.css"
  },
  "dependencies": {
    "@types/dat.gui": "^0.7.7",
    "@types/express": "^4.17.13",
    "@types/fabric": "^4.5.7",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.23",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux": "^3.6.0",
    "@types/sass": "^1.43.1",
    "@types/three": "^0.141.0",
    "browser-sync": "^2.27.9",
    "css-loader": "^6.7.1",
    "dat.gui": "^0.7.9",
    "esbuild": "^0.14.34",
    "esbuild-register": "^3.3.2",
    "express": "^4.17.3",
    "fabric": "^5.2.1",
    "file-loader": "^6.2.0",
    "html2canvas": "^1.4.1",
    "react": "^18.0.0",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "sass": "^1.50.0",
    "sass-loader": "^13.0.0",
    "tailwindcss": "^3.1.4",
    "texttip": "^0.1.0",
    "three": "^0.141.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "antd": "^4.19.5",
    "classnames": "^2.3.1",
    "concurrently": "^7.1.0",
    "esbuild-sass-plugin": "^2.2.6",
    "prettier": "^2.6.2",
    "react-dom": "^18.0.0",
    "react-query": "^3.34.19"
  }
}
